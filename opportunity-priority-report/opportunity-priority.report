{
    "query": "SELECT OpportunityPriority.OPPORTUNITYPRIORITY_NAME as \"Priority\", COUNT(Opportunity.OPPORTUNITY_ID) as \"Total Opportunities\", SUM(Opportunity.OPPORTUNITY_AMOUNT) as \"Total Amount\", Currency.CURRENCY_CODE as \"Currency\"\nFROM CODBEX_OPPORTUNITYPRIORITY as OpportunityPriority\n  INNER JOIN CODBEX_CURRENCY Currency ON Opportunity.OPPORTUNITY_CURRENCY = Currency.CURRENCY_ID\n  INNER JOIN CODBEX_CURRENCY Currency ON Opportunity.OPPORTUNITY_CURRENCY = Currency.CURRENCY_ID",
    "name": "OpportunityPriority",
    "alias": "OpportunityPriority",
    "table": "CODBEX_OPPORTUNITYPRIORITY",
    "columns": [
        {
            "table": "OpportunityPriority",
            "alias": "Priority",
            "name": "OPPORTUNITYPRIORITY_NAME",
            "type": "VARCHAR",
            "aggregate": "NONE",
            "select": true,
            "grouping": false
        },
        {
            "table": "Opportunity",
            "alias": "Total Opportunities",
            "name": "OPPORTUNITY_ID",
            "type": "DECIMAL",
            "aggregate": "COUNT",
            "select": true,
            "grouping": false
        },
        {
            "table": "Opportunity",
            "alias": "Total Amount",
            "name": "OPPORTUNITY_AMOUNT",
            "type": "DOUBLE",
            "aggregate": "SUM",
            "select": true,
            "grouping": false
        },
        {
            "table": "Currency",
            "alias": "Currency",
            "name": "CURRENCY_CODE",
            "type": "VARCHAR",
            "aggregate": "NONE",
            "select": true,
            "grouping": false
        }
    ],
    "joins": [
        {
            "alias": "Currency",
            "name": "CODBEX_CURRENCY",
            "type": "INNER",
            "condition": "Opportunity.OPPORTUNITY_CURRENCY = Currency.CURRENCY_ID"
        },
        {
            "alias": "Currency",
            "name": "CODBEX_CURRENCY",
            "type": "INNER",
            "condition": "Opportunity.OPPORTUNITY_CURRENCY = Currency.CURRENCY_ID"
        }
    ]
}